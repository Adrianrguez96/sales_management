}

/* Estilo para los elementos de la tabla al pasar el ratón */
QTableWidget::item:hover, QTableView::item:hover {
    background-color: #333333; /* Color de fondo al pasar el ratón sobre un elemento de la tabla */
}

/* Estilo para los mensajes de error */
QErrorMessage {
    background-color: #F44336; /* Fondo de mensajes de error */
    color: #FFFFFF; /* Texto en mensajes de error */
}

/* Estilo para CheckBox */
QCheckBox {
    color: #FFFFFF; /* Texto en CheckBox */
}

QCheckBox::indicator:unchecked {
    background-color: #2A2A2A; /* Fondo del CheckBox no marcado */
    border: 1px solid #333333; /* Borde del CheckBox no marcado */
}

QCheckBox::indicator:checked {
    background-color: #4CAF50; /* Fondo del CheckBox marcado */
}

/* Estilo para RadioButton */
QRadioButton {
    color: #FFFFFF; /* Texto en RadioButton */
}

QRadioButton::indicator:unchecked {
    background-color: #2A2A2A; /* Fondo del RadioButton no marcado */
    border: 1px solid #333333; /* Borde del RadioButton no marcado */
}

QRadioButton::indicator:checked {
    background-color: #4CAF50; /* Fondo del RadioButton marcado */
}
