}

/* Estilo para QLabel */
QLabel {
    color: #B0B0B0; /* Texto secundario */
}

/* Estilo para las listas (QListWidget, QListView) */
QListWidget, QListView {
    background-color: #1E1E1E; /* Fondo de la lista */
    color: #FFFFFF; /* Texto en la lista */
    border: 1px solid #333333; /* Borde de la lista */
}

/* Estilo para elementos seleccionados en listas */
QListWidget::item:selected, QListView::item:selected {
    background-color: #4CAF50; /* Color de fondo del elemento seleccionado */
    color: #FFFFFF; /* Color del texto seleccionado */
}

/* Estilo para las barras de progreso (QProgressBar) */
QProgressBar {
    background-color: #2A2A2A; /* Fondo de la barra de progreso */
    border: 1px solid #333333; /* Borde de la barra de progreso */
    border-radius: 5px; /* Bordes redondeados */
}

QProgressBar::chunk {
    background-color: #4CAF50; /* Color de la parte llena de la barra de progreso */
}

/* Estilo para las ComboBox */
QComboBox {
    background-color: #2A2A2A; /* Fondo de ComboBox */
    color: #FFFFFF; /* Texto en ComboBox */
    border: 1px solid #333333; /* Borde de ComboBox */
    padding: 5px; /* Espaciado interno */
    border-radius: 3px; /* Bordes redondeados */
}

/* Estilo para el menú desplegable en ComboBox */
QComboBox QAbstractItemView {
    background-color: #1E1E1E; /* Fondo del menú desplegable */
    color: #FFFFFF; /* Texto en el menú desplegable */
}

/* Estilo para elementos seleccionados en ComboBox */
QComboBox QAbstractItemView::item:selected {
    background-color: #4CAF50; /* Color de fondo del elemento seleccionado */
    color: #FFFFFF; /* Color del texto seleccionado */
}

/* Estilo para las barras de estado (QStatusBar) */
QStatusBar {
    background-color: #1E1E1E; /* Fondo de la barra de estado */
    color: #B0B0B0; /* Texto en la barra de estado */
}

/* Estilo para cuadros de mensaje (QMessageBox) */
QMessageBox {
    background-color: #1E1E1E; /* Fondo de cuadros de mensaje */
    color: #FFFFFF; /* Texto en cuadros de mensaje */
}

/* Estilo para los iconos (QIcon) */
QPushButton::icon {
    padding-right: 5px; /* Espaciado entre el icono y el texto */
}

/* Estilo para tablas (QTableWidget, QTableView) */
QTableWidget, QTableView {
    background-color: #1E1E1E; /* Fondo de la tabla */
    color: #FFFFFF; /* Texto en la tabla */
    border: 1px solid #333333; /* Borde de la tabla */
}

QTableWidget::item:selected, QTableView::item:selected {
