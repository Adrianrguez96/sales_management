}

/* Estilo para botones (QPushButton) */
QPushButton {
    background-color: #4CAF50; /* Color de fondo de botones */
    color: #FFFFFF; /* Texto en botones */
    border: none; /* Sin bordes */
    padding: 10px; /* Espaciado interno */
    border-radius: 5px; /* Bordes redondeados */
    min-width: 100px; /* Ancho mínimo */
}

/* Estilo para botones al pasar el ratón */
QPushButton:hover {
    background-color: #388E3C; /* Color de fondo al pasar el ratón */
}

/* Estilo para botones deshabilitados */
QPushButton:disabled {
    background-color: #555555; /* Color para botones deshabilitados */
    color: #B0B0B0; /* Texto deshabilitado */
}

/* Estilo para campos de texto (QLineEdit, QTextEdit) */
